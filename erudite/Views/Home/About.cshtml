
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">



    <style>
        .container-fluid {
            border: 8px solid white;
            background-color: white;
        }

        .heading {
            background-image: linear-gradient(to right, rgba(160, 20, 80, 0.9),rgba(100, 0, 50, 0.9));
            height: 90px;
            text-align: center;
        }

        h1 {
            padding-left: 25%;
            padding-top: 20px;
            font-family: 'Kaushan Script', cursive;
            letter-spacing: 1mm;
            font-size: 50px;
            color: rgba(12, 8, 3, 0.945);
            font-weight: bold;
        }

        .headerList ul {
            text-decoration: none;
            float: right;
            margin: auto 0;
            margin-left: auto;
            margin-right: 2%;
        }

            .headerList ul li {
                display: inline-block;
            }

        .headerList {
            background-image: linear-gradient(to right, rgba(130, 20, 80, 0.6),rgba(93, 0, 30, 0.9));
            height: 100px;
        }

            .headerList ul li a {
                color: whitesmoke;
                font-weight: bold;
                font-size: 1.6em;
                font-family: Arial, Helvetica, sans-serif;
                margin-left: 25px;
            }

        .student {
            background-image: linear-gradient(to right, rgba(130, 20, 80, 0.6),rgba(93, 0, 30, 0.9));
            color: black;
            font-size: 20px;
        }

        h3 {
            color: rgb(226, 223, 216);
            padding-left: 10px;
            font-weight: bold;
        }

        .btn {
            background-image: linear-gradient(rgb(218, 194, 178), rgb(228, 153, 184));
            border-color: rgb(63, 1, 20);
            font-weight: bold;
            color: rgb(46, 3, 16);
            border-radius: 35px;
            border-width: 3px;
            font-size: 15px;
            letter-spacing: 1mm;
            margin-left: 80px;
        }

        .pink {
            background-image: linear-gradient( rgba(150, 20, 80, 0.9),rgba(93, 0, 50, 0.9));
            color: papayawhip;
        }

        .footer {
            background-image: linear-gradient( rgba(160, 20, 80, 0.9),rgba(100, 0, 50, 0.9));
            color: white;
        }

        .lw {
            background-color: black;
            color: white;
            text-align: center;
            padding-left: 500px;
            padding-top: 10px;
            padding-bottom: 10px;
            padding-right: 500px;
        }.student.ml-5{
            border:none;
            transform:scale(1);
             transition:0.5s ease-in-out;
        }
        .student.ml-5:hover {
            border: 7px solid rgb(241, 191, 104);
            transform: scale(1.1);
        }

        .row.w3-content.w3-section img {
            width: 100%;
            height: 500px;
            object-fit: cover;
        }
    </style>
</head>
<body>
    <div class="container-fluid">


        <div class="row  headerList">
